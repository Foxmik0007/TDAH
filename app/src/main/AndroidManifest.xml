        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TDAHProject">
        <activity android:name="com.volanadesign.tdahproject.partnerSelect"></activity>
        <activity android:name="com.volanadesign.tdahproject.MessageActivity" />
