        </activity>
        <activity android:name="com.volanadesign.tdahproject.loadingToLobby" />
        <activity android:name="com.volanadesign.tdahproject.Registration" />
        <activity android:name="com.volanadesign.tdahproject.Login" />
        <activity android:name="com.volanadesign.tdahproject.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>